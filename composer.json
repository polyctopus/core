{
  "name": "polyctopus/core",
  "description": "Core library for polyctopus multidimensional content",
  "type": "library",
  "version": "1.0.0",
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "psr-4": {
      "Polysync\\Core\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.8"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
